

from django.shortcuts import redirect, render
import random
from accounts.models import Random_Link
from django.db.models import Max, Min



def button(request):
    rand = random.randint(Random_Link.objects.all().aggregate(Max("id")),
                    Random_Link.objects.all().aggregate(Min("id")))
    q = Random_Link.objects.all().filter(id=rand)
    return q.link